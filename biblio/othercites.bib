doi = {10.1063/1.1988986},
issn = {0003-6951},
journal = {Applied Physics Letters},
keywords = {Ge-Si alloys,buffer layers,dislocations,elemental semiconductors,functionally graded materials,semiconductor growth,semiconductor materials,silicon,surface roughness,transmission electron microscopy},
month = {jul},
number = {1},
pages = {012104},
publisher = {American Institute of Physics},
title = {{Surface roughness and dislocation distribution in compositionally graded relaxed SiGe buffer layer with inserted-strained Si layers}},
url = {http://aip.scitation.org/doi/10.1063/1.1988986},
volume = {87},
year = {2005}
}
