keywords = {AlP quantum wells,Magnetoresistance,Scattering time,Temperature effect},
language = {english},
month = {mar},
pages = {84--88},
title = {{Transport properties of the two-dimensional electron gas in wide AlP quantum wells including temperature and correlation effects}},
url = {http://www.sciencedirect.com/science/article/pii/S1386947714004202},
volume = {67},
year = {2015}
}
